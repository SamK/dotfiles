# zsh vi mode: disable ESC delay when switch to normal mode (default: 40 = 0.4sec)
export KEYTIMEOUT=10
LANG=en_US.UTF-8
export VISUAL=vi
export EDITOR=vi
export MOLECULE_NO_LOG=false
export ANSIBLE_STDOUT_CALLBACK=yaml
export ANSIBLE_COLOR_DEBUG=blue
export ANSIBLE_FORCE_COLOR=1
export LESS="-RFXi"
