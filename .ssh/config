# stay connected 30 min
ConnectTimeout=15
TCPKeepAlive=yes
ServerAliveCountMax=120
ServerAliveInterval=15
StrictHostKeyChecking=accept-new
# ssh complection:
HashKnownHosts=no
Include config.d/*.conf

#FIXME: refaire la clé
PubkeyAcceptedKeyTypes +ssh-rsa

# DD-WRT http://localhost:4567/Assets/wnr2000.md
Host wnr2000 192.168.4.1
  User root
  KexAlgorithms +diffie-hellman-group1-sha1
  HostKeyAlgorithms +ssh-dss

# A cause des VMs ça déconne
Host localhost
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null

Host docker01
  User debian

Host jump1
  Hostname wnr2000
  Port 11022
